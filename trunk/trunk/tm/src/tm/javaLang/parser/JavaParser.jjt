PARSER_BEGIN(JavaParser)

package tm.javaLang.parser;

import java.util.* ;
import java.io.Reader ;

import tm.clc.analysis.IdTable ;
import tm.clc.analysis.ScopedName ;
import tm.clc.parser.* ;
